{
    "name": "christhomas/floquent",
    "description": "Define model fields, php attributes on eloquent models, create validators for specific fields",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Thomas",
            "email": "chris.thomas@antimatter-studios.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Floquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Floquent\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.24",
        "laravel/database": "^8.53"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Floquent\\FloquentServiceProvider"
            ]
        }
    }
}
