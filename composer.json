{
    "name": "christhomas/eloquent-fluent",
    "description": "Define model fields, php attributes on eloquent models, create validators for specific fields",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Thomas",
            "email": "chris.thomas@antimatter-studios.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Eloquent\\Fluent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eloquent\\Fluent\\Tests\\": "tests/",
            "Eloquent\\Fluent\\Tests\\Models\\": "tests/Pest/Models"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.24"
    },
    "require-dev": {
        "orchestra/testbench": "^6.9",
        "nunomaduro/larastan": "^0.6.10",
        "pestphp/pest": "^1.2",
        "pestphp/pest-plugin-laravel": "^1.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eloquent\\Fluent\\EloquentFluentServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "stan": "./vendor/bin/phpstan analyse"
    }
}